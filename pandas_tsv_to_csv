# import pandas
import pandas as pd

#define the file (in this case from IMDBs database)
tsv_file = 'name.basics.tsv'

# reading given tsv file using pandas read table
csv_table = pd.read_table(r'###path_on_your_machine###name.basics.tsv', sep='\t')

# converting tsv file into csv
csv_table.to_csv('name.basics.csv', index=False)
